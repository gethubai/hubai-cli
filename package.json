{
  "name": "hubai-cli",
  "version": "0.0.1-alpha",
  "description": "HubAI CLI for creating extensions and brains",
  "type": "module",
  "files": [
    "!lib/__tests__/**/*",
    "lib/**/*",
    "bin/**/*",
    "src/brain/templates/**/*",
    "src/extension/templates/**/*"
  ],
  "bin": {
    "hubai": "./bin/index.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./lib/",
    "cm": "cz",
    "lint": "eslint ./src/ --fix --ignore-pattern src/brain/templates/*",
    "start": "TS_NODE_FILES=true ts-node ./src/cli.ts",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "test:watch": "jest --watch",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gethubai/hubai-cli.git"
  },
  "license": "MIT",
  "author": {
    "name": "Matheus Diniz",
    "email": "mtsdnz@hotmail.com",
    "url": "https://github.com/mtsdnz"
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "hubai",
    "cli",
    "template",
    "brain",
    "extension"
  ],
  "bugs": {
    "url": "https://github.com/gethubai/hubai-cli/issues"
  },
  "homepage": "https://github.com/gethubai/hubai-cli#readme",
  "dependencies": {
    "archiver": "^5.3.1",
    "commander": "^7.2.0",
    "esbuild": "^0.18.17",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.2.9",
    "joi": "^17.9.2",
    "jwt-decode": "^3.1.2",
    "keytar": "^7.9.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@types/archiver": "^5.3.2",
    "@types/ejs": "^3.1.2",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^12.20.11",
    "@types/shelljs": "^0.8.12",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chalk": "^4.1.1",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "ejs": "^3.1.9",
    "esbuild-plugin-replace": "^1.4.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^6.0.0",
    "jest": "^27.2.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.2.1",
    "semantic-release": "^21.0.1",
    "shelljs": "^0.8.5",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "!(src/brain/templates/**/*|src/extension/templates/**/*)*.ts": "eslint --fix"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "type": "build",
              "scope": "deps",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Bug Fixes"
              },
              {
                "type": "build",
                "section": "Dependencies and Other Build Updates",
                "hidden": false
              }
            ]
          }
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
